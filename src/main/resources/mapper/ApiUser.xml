<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ai.maum.mcl.skins.api.apiuser.mapper.ApiUserMapper">
    <select id="getApiUserById" resultType="ai.maum.mcl.skins.api.apiuser.model.ApiUser" parameterType="String">
        SELECT vendor_id, api_key, use_yn, reg_id, created_at, updated_at
        FROM api_users
        where vendor_id = #{vendorId}
    </select>
</mapper>
